.layout-main{
  height: 1150rpx;
  width: 750rpx;
}
.dropDownpage{
  width: 100%;
  height: 10%;
  z-index: 10;
  background-color: red;
}
.modify-divideLine-light{
  background-color: rgba(55,121,205,0.12);
}
.modify-divideLine-dark{
  background-color: rgba(55,121,205,0.38);
}
.modify-shadow-main-down{
  box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0,0,0,0.38);
}
.modify-shadow-main-up{
  box-shadow: 0rpx -2rpx 4rpx 0rpx rgba(0,0,0,0.38);
}
.modify-shadow-button{
  box-shadow: 2rpx 2rpx 2rpx 0rpx rgba(0,0,0,0.38);
}