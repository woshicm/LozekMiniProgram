<view class='backlayer'>
  <view class='imageCard' style='height:{{height}}rpx;'>
    <image class='image' src='{{imageURL}}' mode='aspectFix'></image>
    <view class='information'>
      <image src='/images/icon/ic_location_on.svg' class='icon-locationOn'></image>
      <text class='message'>{{location}}</text>
    </view>
  </view>
</view>